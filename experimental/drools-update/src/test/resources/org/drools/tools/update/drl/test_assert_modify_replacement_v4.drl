package org.drools.test;

import org.drools.Cheese;

rule "test1"
	salience 10
	no-loop true
	when
        cheese : Cheese()
    then
    	retract(cheese);
		insert(cheese);
end


rule "test2"
	salience 0
	no-loop true
	when
        cheese : Cheese()
    then
        update(cheese);
		retract(cheese);
end


