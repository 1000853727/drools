package org.kie.games.adventures

import org.kie.games.adventures.Room;
import org.kie.games.adventures.Thing;

dialect "mvel"

rule printEvent salience 10 when
    $ge : GameEvent()
    SessionCharacter( $us : session )    
then
    $us.channels["events"].send( $ge );
end


rule removeGameEvent when
    $ge : GameEvent()
then
    retract ( $ge );
end