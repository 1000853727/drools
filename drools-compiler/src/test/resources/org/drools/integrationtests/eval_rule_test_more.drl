package org.drools.test

import org.drools.Cheese
import org.drools.Person

global java.util.List list
 

rule "another test"
    when
        p : Person()
        eval(p.getName().equals("foo") && p.getName().startsWith("f"))
    then
        list.add( p );
end  

rule "yet more"
    when
        p : Person()
        eval(p.getName().equals("foo") && p.getName().startsWith("f"))
        eval(p.getName().equals("foo") && p.getName().startsWith("q"))        
    then
        list.add( p );
end  