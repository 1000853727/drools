package org.kie.integrationtests

import org.kie.BinTask;
import org.kie.Bin;

global java.util.List list;

rule "escapeon"
    when
        $task : BinTask( $bn : bin.name matches "\\w\\w\\d{3}\\w\\w" )
        $bin : Bin( $nm : name  matches "\\w\\w\\d{3}\\w\\w" )
    then
        list.add( $task );
end
