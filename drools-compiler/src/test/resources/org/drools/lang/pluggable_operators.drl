package org.drools;

rule "test pluggable operators"
when
    $a : EventA()
    $b : EventB( this after[1,10] $a || this not after[15,20] $a )
    $c : EventC( this finishes $b )
    $d : EventD( this not starts $a )
    $e : EventE( this not before[1, 10] $b || after[1, 10] $c && this after[1, 5] $d )
then
end