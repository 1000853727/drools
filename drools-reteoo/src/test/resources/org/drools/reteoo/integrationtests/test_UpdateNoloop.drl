package org.drools.compiler.test;

import org.drools.compiler.Cheese;

rule "test1"
    salience 10
    no-loop true
    when
        cheese : Cheese( price == 15 )
    then
        update(cheese);
end


rule "test2"
    salience 0
    no-loop true
    when
        cheese : Cheese( price == 15 )
    then
        cheese.setPrice( 14 );
        update(cheese);
end


